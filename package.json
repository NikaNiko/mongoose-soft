{
  "name": "mongoose-soft",
  "version": "0.5.0",
  "description": "Mongoose soft delete",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore ./",
    "ava": "ava -vs test/**/*.test.js",
    "test": "nyc npm run ava && nyc report --reporter=text-lcov | coveralls",
    "test:dev": "nyc npm run ava && nyc report --reporter=html"
  },
  "dependencies": {
    "lodash.assign": "4.2.0"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "coveralls": "2.13.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-import": "2.2.0",
    "mongoose": "4.9.8",
    "nyc": "10.3.2"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kozzztya/mongoose-soft.git"
  },
  "keywords": [
    "mongoose",
    "soft",
    "delete",
    "remove"
  ],
  "author": "kozzztya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kozzztya/mongoose-soft/issues"
  },
  "homepage": "https://github.com/kozzztya/mongoose-soft#readme"
}

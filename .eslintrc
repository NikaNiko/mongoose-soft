{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "worker": true
  },
  "extends": "airbnb-base",
  "rules": {
    "quotes": 0,
    "max-len": [2, {"code": 120, "tabWidth": 2}],
    "handle-callback-err": 2,
    "import/no-extraneous-dependencies": 0,
    "arrow-parens": 0,
    "object-property-newline": 0,
    "no-mixed-operators": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "prefer-const": 0,
    "padded-blocks": 0,
    "no-shadow": 0,
    "no-return-assign": 0,
    "prefer-template": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-eval": 0
  }
}
